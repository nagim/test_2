{
 "gitbook": "2.x.x",
    "plugins": [
        "comment",
        "include",
        "-sharing",
        "anchor-navigation",
        "heading-anchors",
        "ungrey",
        "toggle-chapters",
        "book-summary-scroll-position-saver",
        "splitter",
        "versions",
        "richquotes",
        "copy-code-button",
        "codesnippet",
        "sections",
        "callouts",
        "puml",
        "sectionx"
    ],
    "pluginsConfig": {
        "versions": {
            "type": "branches"
        },
        "richquotes": {
            "danger": {
                "alert": "danger",
                "picto": "fa-exclamation-triangle"
            },
            "warning": {
                "alert": "warning",
                "picto": "fa-exclamation-circle"
            }
        }
    },
    "variables": {
        "note": "<div class=\"alert alert-info\" role=\"alert\"><i class=\"fa fa-info-circle\" </i> <b>Note: </b>"
    }
}